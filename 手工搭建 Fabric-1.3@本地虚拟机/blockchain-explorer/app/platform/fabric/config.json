{
  "network-configs": {
    "dev": {
      "version": "1.0",
      "clients": {
        "client": {
          "tlsEnable": true,
          "organization": "Org1MSP",
          "channel": "mychannel",
          "credentialStore": {
            "path": "./tmp/credentialStore_Org1/credential",
            "cryptoStore": {
              "path": "./tmp/credentialStore_Org1/crypto"
            }
          }
        }
      },
      "channels": {
        "mychannel": {
          "peers": {
            "peer0.org1.alcor.com": {}
          },
          "connection": {
            "timeout": {
              "peer": {
                "endorser": "6000",
                "eventHub": "6000",
                "eventReg": "6000"
              }
            }
          }
        }
      },
      "organizations": {
        "Org1MSP": {
          "mspid": "Org1MSP",
          "fullpath": false,
          "adminPrivateKey": {
            "path": 
              "/root/blockchain-explorer/examples/dockerConfig/crypto/peerOrganizations/org1.alcor.com/users/Admin@org1.alcor.com/msp/keystore"
          },
          "signedCert": {
            "path": 
              "/root/blockchain-explorer/examples/dockerConfig/crypto/peerOrganizations/org1.alcor.com/users/Admin@org1.alcor.com/msp/signcerts"
          }
        },
        "OrdererMSP": {
          "mspid": "OrdererMSP",
          "adminPrivateKey": {
            "path": "/root/blockchain-explorer/examples/dockerConfig/crypto/ordererOrganizations/alcor.com/users/Admin@alcor.com/msp/keystore"
          },
          "signedCert": {
            "path": "/root/blockchain-explorer/examples/dockerConfig/crypto/ordererOrganizations/alcor.com/users/Admin@alcor.com/msp/signcerts"
          }
        }
      },
      "peers": {
        "peer0.org1.alcor.com": {
          "tlsCACerts": {
            "path": "/root/blockchain-explorer/examples/dockerConfig/crypto/peerOrganizations/org1.alcor.com/peers/peer0.org1.alcor.com/tls/ca.crt"
          },
          "url": "grpcs://peer0.org1.alcor.com:7051",
          "eventUrl": "grpcs://peer0.org1.alcor.com:7053",
          "grpcOptions": {
            "ssl-target-name-override": "peer0.org1.alcor.com"
          }
        }
      },
      "orderers": {
        "orderer.alcor.com": {
          "url": "grpcs://orderer.alcor.com:7050"
        }
      }
    },
    "configtxgenToolPath": "/root/blockchain-explorer/bin",
    "license": "Apache-2.0"
  }
}